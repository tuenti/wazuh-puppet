  <!-- Client Authentication Settings -->
  <auth>
    <disabled><%= ossec_auth_enabled ? 'no' : 'yes'%></disabled>
    <port><%= @ossec_auth_port %></port>
    <use_source_ip><%= ossec_auth_use_source_ip ? 'yes' : 'no' %></use_source_ip>
    <force_insert><%= ossec_auth_force_insert ? 'yes' : 'no' %></force_insert>
    <force_time><%= @ossec_auth_force_time %></force_time>
    <purge><%= ossec_auth_purge ? 'yes' : 'no' %></purge>
    <use_password><%= defined?($ossec_auth_agent_password) ? 'yes' : 'no' %></use_password>
    <ssl_agent_ca>#TODO</ssl_agent_ca>
    <ssl_verify_host><%= ossec_auth_ssl_verify_host ? 'yes' : 'no' %></ssl_verify_host>
    <ssl_manager_cert><%= @ossec_auth_ssl_manager_cert %></ssl_manager_cert>
    <ssl_manager_key><%= @ossec_auth_ssl_manager_key %></ssl_manager_key>
    <ssl_auto_negotiate><%= ossec_auth_ssl_auto_negotiate ? 'yes' : 'no' %></ssl_auto_negotiate>
    <ciphers><%= @ossec_auth_ciphers %></ciphers>
    <limit_maxagents><%= ossec_auth_limit_maxagents ? 'yes' : 'no' %></limit_maxagents>
  </auth>


